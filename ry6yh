""" 
def factors(x):
    divisors = []
    for i in range(1, x + 1):
        if x % i == 0:
            divisors.append(i)  # Add i to the list if it's a divisor
print(factors(5)) """

def find_factors(number):
    factors = []
    for i in range(1, number + 1):
        if number % i == 0:
            factors.append(i)
    return factors

# Example usage:
number = int(input("Enter a number: "))
print("Factors of", number, "are:", find_factors(number))